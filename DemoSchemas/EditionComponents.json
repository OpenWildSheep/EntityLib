        "StaticObjectGD": {
            "properties": {
                "Super": {
                    "$ref": "./RuntimeComponents.json#/definitions/ComponentGD"
                },
                "EDITOR_UsedInMultiLayerHeightmap": {
                    "type": "boolean",
                    "default": true,
					"description": "Determines, if this object is static, if it should be merged with the terrain when generating the multi-layer height map (only works if this static object has mesh colliders)"
                },
                "EDITOR_MergeInNavMesh": {
                    "type": "boolean",
                    "default": true,
					"description": "Determines, if this object is static, if it should be merged with the terrain when generating the nav mesh (only works if this static object's nav mesh stamper is active and has actual stampers set)"
                },
                "EDITOR_IsCave": {
                    "type": "boolean",
                    "default": false,
					"description": "By default, static objects merged with the terrain have their faces culled, when found under the terrain - this flag prevents that for static meshes that we know represent underground elements like caves"
                },
                "EDITOR_IsUnclosedMesh": {
                    "type": "boolean",
                    "default": false,
					"description": "This tells the tools that this mesh is intended to stay opened (otherwise tools by default force meshes to be closed for optimizations)"
                }
            }
        },
