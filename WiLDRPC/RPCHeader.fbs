namespace WildRPC;

enum ElementType : ubyte { Boolean = 0, Integer, Float, Vector2, UInt3, Vector3, Quat, Color, Position, String }
enum TypeInfo : ushort (bit_flags) { Array, Boolean, Integer, Float, Vector2, UInt3, Vector3, Quat, Color, Position, String }

table RPCHeader
{
	managerName:string (required);
	methodName:string (required);
	parameterTypes:[TypeInfo] (required);
	resultTypes:[TypeInfo] (required);
}

root_type RPCHeader;
