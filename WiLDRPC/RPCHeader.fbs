include "Result.fbs";

namespace WildRPC;

enum Type : byte { UInt3 = 1, Vector3, Vector2, Quat, Position, Float, Integer, Boolean, Color }

table RPCHeader
{
	managerName:string (required);
	methodName:string (required);
	parameterTypes:[Type] (required);
	resultTypes:[Type] (required);
}

root_type RPCHeader;