include "Result.fbs";

namespace WildRPC;

enum Type : byte { Boolean = 0, Integer, Float, Vector2, UInt3, Vector3, Quat, Color, Position, String }

table RPCHeader
{
	managerName:string (required);
	methodName:string (required);
	parameterTypes:[Type] (required);
	resultTypes:[Type] (required);
}

root_type RPCHeader;